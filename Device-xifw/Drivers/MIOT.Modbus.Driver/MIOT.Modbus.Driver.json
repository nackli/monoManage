{
  "className": "MIOT.Modbus.Driver",
  "driverDesc": "Modbus协议驱动包",
  "parentName": "MODBUS",
  "version": "1.0.1",
  "protocols": [
    {
      "protocolName": "Modbus Tcp",
      "tagSource": 0,
      "connectParam": [
        {
          "name": "ip",
          "desc": "IP地址",
          "formType": "input",
          "required": true,
          "default_value": "127.0.0.1"
        },
        {
          "name": "port",
          "desc": "端口",
          "formType": "input",
          "required": true,
          "default_value": "502",
          "rules": [
            {
              "pattern": "^\\d{1,5}$",
              "message": "请输入 0~65535 的数字"
            }
          ]
        },
        {
          "name": "station_id",
          "desc": "站号",
          "formType": "input",
          "required": true,
          "default_value": "1"
        },
        {
          "name": "endian",
          "desc": "字节顺序",
          "formType": "select",
          "required": false,
          "default_value": "CDAB",
          "option": [
            {
              "value": "ABCD",
              "label": "ABCD"
            },
            {
              "value": "BADC",
              "label": "BADC"
            },
            {
              "value": "CDAB",
              "label": "CDAB"
            },
            {
              "value": "DCBA",
              "label": "DCBA"
            }
          ]
        },
        {
          "name": "start_zero",
          "desc": "首地址从0开始",
          "formType": "checkbox",
          "required": false
        },
        {
          "name": "batchRead",
          "desc": "批量读取",
          "formType": "checkbox",
          "required": false
        },
        {
          "name": "isStringReverse",
          "desc": "字符串颠倒",
          "formType": "checkbox",
          "required": false
        },
        {
          "name": "conn_timeout",
          "desc": "连接超时(毫秒)",
          "formType": "input",
          "required": true,
          "default_value": "5000"
        }
      ]
    },
